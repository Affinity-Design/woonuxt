{
  "name": "woonuxt",
  "private": true,
  "scripts": {
    "build": "node scripts/build-sitemap.js && nuxt build && node scripts/setup-script.js",
    "build:local": "node scripts/build-sitemap.js && nuxt build --config nuxt.config.local.ts",
    "preview:local": "nuxt preview --config nuxt.config.local.ts",
    "dev": "nuxt dev --host",
    "dev:ssl": "nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "node scripts/build-sitemap.js && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "nuxi generate && npx serve .output/public -l 8080",
    "setup-cache": "node scripts/setup-script.js",
    "build-product-cache": "node scripts/build-products-cache.js",
    "build-category-cache": "node scripts/build-categories-cache.js",
    "build-blog-routes": "node scripts/build-blog-routes.js",
    "build-all-routes": "node scripts/build-all-routes.js",
    "build-sitemap": "node scripts/build-sitemap.js",
    "build-sitemap:local": "node scripts/build-sitemap.js",
    "warm-cache": "node scripts/cache-warmer.js all",
    "warm-products": "node scripts/cache-warmer.js products",
    "warm-categories": "node scripts/cache-warmer.js categories",
    "warm-home": "node scripts/cache-warmer.js home",
    "rebuild-cache": "npm run setup-cache && node scripts/cache-warmer.js all --force",
    "clear-cache": "node scripts/clear-kv-cache.js",
    "clear-cache-all": "node scripts/clear-kv-cache.js --all",
    "clear-page-cache": "node scripts/clear-kv-cache.js --namespace=cache",
    "clear-data-cache": "node scripts/clear-kv-cache.js --namespace=script_data",
    "reset-cache": "npm run clear-cache-all && npm run setup-cache",
    "clean": "node scripts/clean-build.js",
    "clean:build": "npm run clean && npm install && npm run build",
    "postdeploy": "node scripts/post-deploy.js",
    "verify-env": "node scripts/verify-env.js",
    "debug:cache": "node scripts/debug-cache.js",
    "optimize-images": "node scripts/optimize-images.js",
    "preinstall": "npx only-allow npm"
  },
  "dependencies": {
    "@nuxt/content": "^2.13.4",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/image": "1.9.0",
    "@nuxtjs/i18n": "^8.5.5",
    "@nuxtjs/seo": "^2.0.3",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@sendgrid/mail": "^8.1.4",
    "@stripe/stripe-js": "^4.10.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/lodash-es": "^4.17.12",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^12.7.0",
    "algoliasearch": "^5.21.0",
    "fuse.js": "^7.1.0",
    "instantsearch.css": "^8.5.1",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.13.2",
    "nuxt-graphql-client": "^0.2.43",
    "nuxt-gtag": "^3.0.2",
    "prettier": "^3.5.1",
    "stripe": "^17.7.0",
    "vue-instantsearch": "^4.20.5",
    "vue-spinner": "^1.0.4",
    "vue-turnstile": "^1.0.11",
    "woonuxt-settings": "^1.0.66"
  },
  "devDependencies": {
    "@iconify-json/ion": "^1.2.2",
    "node-fetch": "^2.7.0",
    "sharp": "0.32.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.34.8"
  },
  "overrides": {
    "sharp": "0.32.6"
  }
}
